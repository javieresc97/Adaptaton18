<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="AdaptatonMobile.App">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="Green">#06D6A0</Color>
            <Color x:Key="Pink">#EF476F</Color>
            <Color x:Key="Blue">#26547C</Color>

            <Style TargetType="Button">
                <Setter Property="TextColor" Value="White" />
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>
